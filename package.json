{
  "name": "semantic-release-git-publish",
  "version": "1.0.1",
  "description": "A semantic-release plugin that publishes by pushing the contents of this Git repository to another Git repository",
  "keywords": [
    "semantic-release",
    "git"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:jcapogna/semantic-release-git-publish.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@semantic-release/error": "3.0.0",
    "dir-compare": "^4.2.0",
    "globby": "^13.2.2",
    "simple-git": "^3.22.0",
    "tmp-promise": "^3.0.3"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/semantic-release__error": "^3.0.0",
    "@types/signale": "^1.2.1",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "vitest": "^1.2.2",
    "@vitest/coverage-v8": "^1.2.2"
  },
  "peerDependencies": {
    "semantic-release": ">=18.0.0"
  },
  "author": "Justin Capogna",
  "license": "MIT"
}
